    <stat type="request_time" url = "/_stats/couchdb/request_time">
        <stat type="value" url = "/_stats/couchdb/request_time/value">
            <metric attr="arithmetic_mean" alias="Avg Request Time" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        </stat>
    </stat>
    <stat type="couch_server" url = "/_stats/couchdb/couch_server">
