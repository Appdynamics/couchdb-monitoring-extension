<stats>
    <stat type="couchdb" url = "/_stats/couchdb">
        <metric attr="auth_cache_hits" alias="Number of Cache Hits" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="auth_cache_misses" alias="Number of Cache Misses" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>

    <stat type="httpd" url = "/_stats/couchdb/httpd">
        <metric attr="aborted_requests" alias="Number Aborted Requests" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <stat type="bulk_docs" url = "/_stats/couchdb/httpd/bulk_docs">
            <stat type="value" url = "/_stats/couchdb/httpd/bulk_docs">
                <metric attr="arithmetic_mean" alias="Bulk Docs" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
            </stat>
        </stat>
        <metric attr="bulk_requests" alias="Number of Bulk Requests" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="requests" alias="Number of http Requests" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="temporary_view_reads" alias="Temporary View Reads" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="view_reads" alias="Number of View Reads" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="clients_requesting_changes" alias="Number of Clients Requesting Changes" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        <metric attr="purge_requests" alias="Number of Purge Requests" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
    </stat>

    <stat type="dbinfo" url = "/_stats/couchdb/dbinfo">
        <stat type="value" url = "/_stats/couchdb/dbinfo">
            <metric attr="arithmetic_mean" alias="Avg DB Info Calls" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
        </stat>
    </stat>
<!--    collect_results_time-->
    <metric attr="database_writes" alias="DB Writes" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
    <metric attr="database_reads" alias="DB Reads" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
    <metric attr="database_purges" alias="DB Purge" aggregationType = "OBSERVATION" timeRollUpType = "CURRENT" clusterRollUpType = "INDIVIDUAL"/>
